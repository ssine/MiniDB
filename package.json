  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "electron": "4.0.2",
    "express": "^4.17.1",
    "jison": "^0.4.18",
    "request": "^2.88.0",
    "shx": "^0.3.2",
    "typescript": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ssine/MiniDB.git"
  }
}
