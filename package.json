{
  "name": "minidb",
  "version": "0.0.1",
  "description": "a core implementation of dbms",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm run build && electron ./js/main.js",
    "cc": "jison ./src/parser/sql.y -o ./src/parser/parser.js & xcopy .\\src\\parser\\parser.js .\\js\\parser\\ /Y",
    "dist": "xcopy .\\js\\*.js .\\dist\\ /Y && xcopy .\\js\\parser\\*.js .\\dist\\parser\\ /Y && xcopy .\\node_modules\\xterm .\\dist\\node_modules\\xterm\\ /Y"
  },
  "author": "sine",
  "license": "ISC",
  "dependencies": {
    "electron": "4.0.2",
    "jison": "^0.4.18",
    "typescript": "^3.2.4",
    "xterm": "^3.10.1"
  }
}
