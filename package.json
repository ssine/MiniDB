{
  "name": "minidb",
  "version": "0.0.1",
  "description": "a core implementation of dbms",
  "main": "index.js",
  "scripts": {
    "build": "tsc & shx cp -rf ./src/pages/ ./js/",
    "watch": "tsc -w",
    "start": "npm run build && electron ./js/main/main.js",
